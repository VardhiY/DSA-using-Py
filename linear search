


def search(array, key):
    n = len(array)
    for i in range(0, n):
        if array[i] == key:
            return i
    return -1



array = [2, 3, 4, 10, 40]
x = 10
result = search(array, x)
if result == -1:
    print("Element is not present in array")
else:
    print("Element is present at index", result)

